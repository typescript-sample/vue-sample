{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "jsx": "preserve",

    "outDir": "build/dist",
    "module": "es2015",
    "target": "es5",
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noImplicitThis": true,
    "noImplicitAny": true,
    "paths": {
      "components/*": ["src/components/*"],
      "@/*":["src/*"]
    }
  },
  "include": [
    "./shims-vue.d.ts",
    "./src/**/*.ts",
    "./src/**/*.vue",
    "src/admin/role/PrivilegesForm/index.js", "src/admin/role/PrivilegesForm/PrivilegesItem.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
